<manifest
	xmlns:ml="magicleap"
	ml:package="com.mlexamples.imagetrackerbug"
	ml:version_code="1"
	ml:version_name="1.0">
  <application
		ml:visible_name="ImageTrackerBug"
		ml:sdk_version="0.19.0"
		ml:min_api_level="3">
    <uses-privilege ml:name="WorldReconstruction"/>
    <uses-privilege ml:name="LowLatencyLightwear"/>
    <uses-privilege ml:name="CameraCapture"/>
    <component
			ml:name="ImageTrackerBugComponent"
			ml:visible_name="ImageTrackerBug"
			ml:binary_name="bin/ImageTrackerBug"
			ml:type="Fullscreen">
    </component>
  </application>
</manifest>
