<manifest
	xmlns:ml="magicleap"
	ml:package="com.mlexamples.handtracking"
	ml:version_code="1"
	ml:version_name="1.0">
  <application
		ml:visible_name="HandTracking"
		ml:sdk_version="0.19.0"
		ml:min_api_level="3">
    <uses-privilege ml:name="WorldReconstruction"/>
    <uses-privilege ml:name="LowLatencyLightwear"/>
    <uses-privilege ml:name="GesturesConfig"/>
    <uses-privilege ml:name="GesturesSubscribe"/>
    <component
			ml:name="HandTrackingComponent"
			ml:visible_name="HandTracking"
			ml:binary_name="bin/HandTrackingExample"
			ml:type="Fullscreen">
      <icon ml:model_folder="media/icon/model"
            ml:portal_folder="media/icon/portal"/>
    </component>
  </application>
</manifest>
